#!/bin/bash
# SL
# ==========================================
# Color
RED='\033[0;31m'
NC='\033[0m'
GREEN='\033[0;32m'
ORANGE='\033[0;33m'
BLUE='\033[0;34m'
PURPLE='\033[0;35m'
CYAN='\033[0;36m'
LIGHT='\033[0;37m'
# ==========================================
clear
echo -e ""
echo -e "Starting Restart All Service"
sleep 2
systemctl restart trojan-go
echo -e "Restart All Service Berhasil"
waktune=$(date);
echo "berhasil restart trojan-go " $waktune >> log-restart-trojan;
